<!DOCTYPE html>
<html lang="en-US">

<head>

<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
<meta name="description" content="{{ page_description }}">

<meta name="msapplication-tap-highlight" content="no">

<meta name="application-name" content="{{ site_name }}">

<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="apple-mobile-web-app-title" content="{{ site_name }}">

{% include "components/favicons.njk" %}

<meta property="og:title" content="{{ page_title }}">
<meta property="og:site_name" content="{{ site_name }}">
<meta property="og:type" content="website">
<meta property="og:url" content="{{ home_url }}/{{ page_slug }}">
<meta property="og:image" content="{{ home_url }}/assets/images/share.jpg">
<meta property="og:description" content="{{ page_description }}">
<meta property="og:locale" content="en_US">

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="{{ page_title }}">
<meta name="twitter:site" content="{{ twitter_handle }}">
<meta name="twitter:url" content="{{ home_url }}/{{ page_slug }}">
<meta name="twitter:description" content="{{ page_description }}">
<meta name="twitter:image" content="{{ home_url }}/assets/images/share.jpg">

<link href="https://plus.google.com/+{{ google_plus_url }}" rel="publisher">
<meta itemprop="name" content="{{ page_title }}">
<meta itemprop="description" content="{{ page_description }}">
<meta itemprop="image" content="{{ home_url }}/assets/images/share.jpg">

<link rel="canonical" href="{{ home_url }}/{{ page_slug }}">

<title>{{ page_title }}</title>

<link rel="stylesheet" href="{{ home_url }}/assets/css/main.min.css">

{% if google_analytics_id %}
<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
	ga('create', '{{ google_analytics_id }}', 'auto');
	ga('send', 'pageview');
</script>
{% endif %}

</head>

<body>

{% include "components/header.njk" %}

{% block main %}{% endblock %}

{% include "components/footer.njk" %}

<script src="{{ home_url }}/assets/js/vendors.min.js"></script>
<script src="{{ home_url }}/assets/js/main.min.js"></script>

</body>
</html>
